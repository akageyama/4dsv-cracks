program main
  use const_base_m
  use crackpoints_m
  use vis_m
  implicit none

  type(crackpoints_t) :: crackpoints

  integer :: step

  call const_base__check
  call crackpoints.initialize
  call crackpoints.check
  
  do step = 0, 100
    call crackpoints.read_file
  end do

  call vis.initialize
  call vis.visualize( crackpoints )

end program main

